{
  "manifest_version": 2,
  "name": "Eye Rest reminder+",
  "version": "0.1.0",
  "permissions": 
  [ 
    "storage",
    "notifications",
    "alarms"
  ],
  "background":
  {
    "scripts": ["background.js"]
  },
  "browser_action":
  {
    "default_popup": "popup.html",
    "default_title": "Popup",
    "default_icon": "icon128.png",
    "icons": 
    {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}